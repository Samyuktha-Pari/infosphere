{
  "builds": [
    {
      "src": "backend/app.py",
      "use": "@vercel/python",
      "config": {
        "workdirectory": "backend"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "backend/app.py"
    },
    {
      "src": "/static/(.*)",
      "dest": "backend/static/$1"
    }
  ]
}