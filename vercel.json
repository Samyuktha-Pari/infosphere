{
  "builds": [
    {
      "src": "api/app.py",
      "use": "@vercel/python",
      "config": {
        "workdirectory": "api",
        "includeFiles": [
          "extractor.py",
          "requirements.txt"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/app.py"
    }
  ]
}